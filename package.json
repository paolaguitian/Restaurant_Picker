{
  "name": "restaurant_picker",
  "version": "1.0.0",
  "description": "Not sure where to eat! We will pick for you",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "client": "parcel watch client/index.html",
    "server": "nodemon server/index.js",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "build": "parcel build client/index.html",
    "start": "npm run build && node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paolaguitian/Restaurant_Picker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paolaguitian/Restaurant_Picker/issues"
  },
  "homepage": "https://github.com/paolaguitian/Restaurant_Picker#readme",
  "devDependencies": {
    "@babel/node": "^7.10.5",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "express": "^4.17.3",
    "express-pino-logger": "^5.0.0",
    "node-env-run": "^4.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "pino-colada": "^2.0.1",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0",
    "antd": "^4.4.3",
    "axios": "^0.19.2",
    "babel-cli": "^6.26.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
